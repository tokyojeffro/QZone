<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:key="PREF_QZONE_SETTING"
        android:title="Q-Zone Enabled?"
        android:summary="Select to turn on Q-Zone Service"
        android:defaultValue="true"
        />
    <ListPreference 
        android:key="PREF_SCAN_FREQ"
        android:title="Q-Zone Scan Interval"
        android:summary="Time between Q-Zone Scans"
        android:entries="@array/scan_interval_options"
        android:entryValues="@array/scan_interval_values"
        android:dialogTitle="Scan Interval"
        android:defaultValue="30"
        />
</PreferenceScreen>